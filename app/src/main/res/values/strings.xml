    <string name="app_name">Padland</string>

    <string name="title_activity_intro">Introduction</string>
    <string name="launch_intro">Launch introduction screens</string>
    <string name="intro_first_title">Welcome to Padland!</string>
    <string name="intro_first_desc">Padland allows you to view, edit, organize and share collaborative documents.\n Press next to see how it works!</string>
    <string name="intro_second_title">Create new documents</string>
    <string name="intro_second_desc">Create documents in your favorite server. You can then organize them in groups and tell your friends to join them!</string>
    <string name="intro_third_title">Edit existing documents</string>
    <string name="intro_third_desc">Share a document url to Padland to open it for edition.</string>
    <string name="intro_fourth_title">Custom server?</string>
    <string name="intro_fourth_desc">No problem! You can add your favorite servers. Check the settings for this and many other options!</string>
    <string name="intro_fifth_title">Ready to go!</string>
    <string name="intro_fifth_desc">Request new features or report bugs in the github repository.\n
        <a href='https://github.com/mikifus/padland'>https://github.com/mikifus/padland</a>
        \n\nPress done to start!</string>

